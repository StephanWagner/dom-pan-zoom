{
  "name": "dom-pan-zoom",
  "description": "A lightweight library providing mobile friendly panning and zooming to any HTML DOM element.",
  "version": "0.1.4",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "pan",
    "zoom",
    "pan-zoom"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stephanwagner/dom-pan-zoom.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "prepublishOnly": "npm run build && node test/assets.js"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.54.0"
  }
}
